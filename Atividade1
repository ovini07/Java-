import java.util.Scanner;

public class ativ1 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		String login;
		String senha;
		String cadastroLogin;
		String cadastroSenha;
		// Solicita o login e a senha
		
		System.out.print("cadastre login:");
    	cadastroLogin = ler.next();
    	
		System.out.print("cadastro senha:");
		cadastroSenha = ler.next();
		
		System.out.println(" ");
		System.out.println("Cadastrado com sucesso");
		System.out.println(" ");
				
		
		System.out.print("Digite o login:");
		login = ler.next();
				
		System.out.print("Digite a senha:");
		senha = ler.next();
				// Verifica se os dados est√£o corretos
		
		if (login.equals(cadastroLogin) && senha.equals(cadastroSenha)) {
			
			System.out.println(" ");
			System.out.println("Cadastrado com sucesso");
		} 
		else {
			System.out.println("Dados incorretos, informe os dados novamente");
				}
		ler.close();
			}

		}
